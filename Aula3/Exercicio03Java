
import java.util.Scanner;

public class Exercicio01Java {

    public static void main(String[] args) {
        Scanner leia = new Scanner(System.in);
       int n1;
       int n2;

         System.out.println("Digite Primeiro numero: ");
         n1 = leia.nextInt();
         System.out.println("Digite Segundo numero: ");
         n2 = leia.nextInt();


            if(n1 > n2 || n1 == 0){
                System.out.println("Intervalo invalido! /n Valor inicial sempre maior que o segundo valor");
            }


           for(int contador = n1; contador <= n2; contador ++){
               if(contador % 3 == 0 && contador % 5 == 0)
                   System.out.println(contador + " Ã© multiplo de 5 e 3");

        }
    }


}
